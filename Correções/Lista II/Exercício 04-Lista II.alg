algoritmo "Exercício 4 - Valores Maiores"
// Função :
// Autor :
// Data : 04/05/2021
// Seção de Declarações 
var
valor: inteiro
valorDois: inteiro
inicio
escreval("Digite o 1º valor")
leia(valor)
escreval("Digite o 2º valor")
leia(valorDois)
se(valor > valorDois) entao
escreval("1º valor é maior!")
senao
escreval("2º valor é maior!")
fimse
fimalgoritmo
