// Seção de Declarações 
var
contadora: inteiro
inicio
contadora <- 11

//repita
//escreval(contadora)
//contadora <- contadora + 1
//ate contadora > 10

repita
escreval(contadora)
contadora <- contadora - 1
ate contadora < 0

fimalgoritmo
